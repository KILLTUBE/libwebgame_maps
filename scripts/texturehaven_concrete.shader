textures/texturehaven_concrete/concrete_1k
{
	qer_editorimage textures/texturehaven_concrete/concrete_diff_1k.jpg
	{
		map textures/texturehaven_concrete/concrete_diff_1k.jpg
		rgbgen identity
	}
	{
		stage normalparallaxmap
		map textures/texturehaven_concrete/concrete_1k_n.png
	}
	{
		stage specularmap
		map textures/texturehaven_concrete/concrete_1k_s.jpg
	}
	{
		map $lightmap
		blendfunc GL_DST_COLOR GL_ZERO
	}
}

